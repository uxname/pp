{
  "$schema": "https://uxna.me/kodu/schema.json",
  "llm": {
    "provider": "openai",
    "model": "gpt-4o",
    "apiKeyEnv": "OPENAI_API_KEY"
  },
  "cleaner": {
    "whitelist": ["//!"],
    "keepJSDoc": true
  },
  "packer": {
    "ignore": [
      "package-lock.json",
      "yarn.lock",
      "pnpm-lock.yaml",
      ".git",
      ".kodu",
      "node_modules",
      "dist",
      "coverage"
    ]
  }
}
